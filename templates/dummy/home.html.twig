{% extends 'base.html.twig' %}

{% block title %}Home{% endblock %}

{% block body %}
    {{ include('partials/_navbar.html.twig') }}

    <style>
        .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    </style>

    <div class="example-wrapper">
        <h1>{{ text }}</h1>

        <ul>
            {% for year in years %}
                <li>{{ year }}</li>
            {% endfor %}
        </ul>
    </div>
{% endblock %}